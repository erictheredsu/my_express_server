<mvc:View
	controllerName="UI5Shellproject.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.tnt"
	xmlns:m="sap.m"
	height="100%">
	<m:Button text="Toggle Collapse/Exapand"
			  icon="sap-icon://menu2"
			  press="onCollapseExapandPress" />
	<NavigationList id="navigationList" width="80%">
		<NavigationListItem
				text="Pages"
				icon="sap-icon://building">
			<NavigationListItem select="onPress" text="databinding">
			</NavigationListItem>
			<NavigationListItem select="onPress" text="HelloWorld">
			</NavigationListItem>
			<NavigationListItem select="onPress" text="ThinClientViewer">
			</NavigationListItem>
			<NavigationListItem select="onPress" text="SplitTest">
			</NavigationListItem>
		</NavigationListItem>
		<NavigationListItem
				text="Item 2"
				icon="sap-icon://employee">
			<NavigationListItem select="onPress" text="Sub Item 1">
			</NavigationListItem>
			<NavigationListItem select="onPress" text="Sub Item 2">
			</NavigationListItem>
			<NavigationListItem select="onPress" text="Sub Item 3">
			</NavigationListItem>
			<NavigationListItem select="onPress" text="Sub Item 4">
			</NavigationListItem>
		</NavigationListItem>
	</NavigationList>
	<m:App id="app"
		 busyIndicatorDelay="{appView>/delay}"/>
</mvc:View>